<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nelsonsouza.citycar.Activity_ger_DetalheSolicitacao"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/lblDetalheNome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lblNome"
        tools:text="@string/lblNome"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/txtDetalheNome"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtDetalheNome"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:singleLine="false"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="25dp"
        android:editable="false"
        android:inputType="none"
        app:layout_constraintRight_toLeftOf="@+id/lblDetalheSetor"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/lblDetalheNome"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/lblDetalheSetor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lblSetor"
        tools:text="@string/lblSetor"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="141dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtDetalheSetor"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:ems="10"
        android:inputType="none"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/txtDetalheModelo"
        app:layout_constraintTop_toBottomOf="@+id/lblDetalheSetor"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/lblDetalheSetor" />

    <TextView
        android:id="@+id/lblDetalhePeriodo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_periodo"
        tools:text="@string/txt_periodo"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/txtDetalheNome"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/txtDetalheNome" />

    <EditText
        android:id="@+id/txtDetalhePeriodo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textSize="11sp"
        tools:maxLines="2"
        android:editable="false"
        android:inputType="none"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="10dp"
        app:layout_constraintRight_toRightOf="@+id/txtDetalheMotivo"
        app:layout_constraintTop_toBottomOf="@+id/lblDetalhePeriodo"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/lblDetalhePeriodo" />

    <TextView
        android:id="@+id/lblDetalheMotivo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_motivo"
        tools:text="@string/txt_motivo"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/txtDetalhePeriodo"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/lblDetalheHoraInicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_hora_inicio"
        tools:text="@string/txt_hora_inicio"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/txtDetalheSetor"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/txtDetalheSetor" />

    <Button
        android:id="@+id/btnDetalheSolicitacaoVeiculos"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_width="349dp"
        android:layout_height="44dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="21dp"
        android:onClick="listaVeiculoClicked"
        android:text="@string/btnConsularVeiculos"
        app:layout_constraintBottom_toTopOf="@+id/lblDetalhePlaca"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@+id/txtDetalheModelo"
        app:layout_constraintTop_toBottomOf="@+id/txtDetalheMotivo"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:textSize="12sp"
        app:layout_constraintHorizontal_bias="0.187"
        tools:layout_editor_absoluteY="198dp" />

    <EditText
        android:id="@+id/txtDetalheMotivo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:ems="10"
        android:inputType="none"
        android:textSize="12sp"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/lblDetalheMotivo"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/lblDetalheMotivo" />


    <EditText
        android:id="@+id/txtDetalheHoraInicio"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="27dp"
        android:layout_marginStart="25dp"
        android:editable="false"
        android:ems="10"
        android:inputType="none"
        android:textSize="11sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/txtDetalheSetor"
        app:layout_constraintTop_toBottomOf="@+id/lblDetalheHoraInicio"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/txtDetalheNome" />

    <TextView
        android:id="@+id/lblDetalheVeiculo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lblPlaca"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="231dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="230dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txtDetalheNome" />

    <TextView
        android:id="@+id/lblDetalhePlaca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lblModelo"
        android:layout_marginStart="1dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/txtDetalheModelo"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/txtDetalheModelo" />

    <EditText
        android:id="@+id/txtDetalhePlaca"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:editable="false"
        android:ems="10"
        android:inputType="none"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="2dp"
        app:layout_constraintRight_toRightOf="@+id/btnCancelar"
        app:layout_constraintTop_toBottomOf="@+id/lblDetalheVeiculo"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/lblDetalheVeiculo" />

    <EditText
        android:id="@+id/txtDetalheModelo"
        android:layout_width="177dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:editable="false"
        android:ems="10"
        android:inputType="none"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/txtDetalheSolicitacaoLocalRetirada"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="17dp" />

    <Button
        android:id="@+id/btnCancelar"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:onClick="btnCancelar"
        android:text="@string/btnReprovar"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/btnAprovar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@+id/txtDetalhePeriodo"
        app:layout_constraintTop_toTopOf="@+id/btnAprovar"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/btnAprovar"
        android:layout_width="118dp"
        android:layout_height="48dp"
        android:onClick="btnAprovar"
        android:text="@string/btnAprovar"
        android:textSize="12sp"
        android:layout_marginEnd="16dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="37dp"
        app:layout_constraintTop_toBottomOf="@+id/txtDetalheSolicitacaoHoraRetirada"
        android:layout_marginBottom="49dp" />

    <EditText
        android:id="@+id/txtDetalheSolicitacaoLocalRetirada"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textMultiLine"
        android:textSize="12sp"
        app:layout_constraintRight_toLeftOf="@+id/txtDetalheSolicitacaoHoraRetirada"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="23dp"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lblLocalRetir"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/txtDetalhePlaca"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/txtDetalhePlaca" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="@string/lblHoraRetir"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/txtDetalheModelo"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/txtDetalheModelo" />

    <EditText
        android:id="@+id/txtDetalheSolicitacaoHoraRetirada"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="12sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/txtDetalheModelo"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView15" />

</android.support.constraint.ConstraintLayout>
